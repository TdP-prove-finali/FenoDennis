<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane maxHeight="700.0" maxWidth="1000.0" minHeight="700.0" minWidth="1000.0" prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polito.tesi.GtfsController">
   <top>
      <Label text="Label" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <HBox>
               <children>
                  <DatePicker fx:id="dataPicker" />
                  <ComboBox fx:id="comboOra" prefWidth="150.0" />
                  <ComboBox fx:id="comboMinuti" prefWidth="150.0" />
                  <Button mnemonicParsing="false" onAction="#doSimula" text="Simula" />
               </children>
            </HBox>
            <TextArea fx:id="txtResult" wrapText="true" />
         </children>
      </VBox>
   </center>
</BorderPane>
